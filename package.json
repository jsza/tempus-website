{
  "name": "tempus-website",
  "version": "0.0.0",
  "description": "Tempus Jump Network website client.",
  "main": "index.js",
  "scripts": {
    "watch": "npm run watch-js",
    "build": "npm run build-js && npm run build-styl",
    "build-js": "BUILD_DEV=0 webpack -p --progress --colors --display-error-details --config js/webpack/prod.config.js",
    "watch-js": "webpack-dev-server --open --config webpack/dev.config.js",
    "build-styl": "stylus -c -u autoprefixer-stylus stylus/index.styl -o ../build/website/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsza/tempus-website.git"
  },
  "author": "jayess",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsza/tempus-website/issues"
  },
  "dependencies": {
    "autobahn": "^0.9.7",
    "autoprefixer": "^6.3.6",
    "autoprefixer-stylus": "^0.8.0",
    "babel": "^5.8.23",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^5.8.25",
    "bootstrap": "^4.0.0-beta.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "history": "^4.7.2",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "postcss-loader": "^0.9.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-custom-scrollbars": "^4.2.0",
    "react-document-title": "^2.0.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-immutable-proptypes": "^1.7.1",
    "react-lazyload": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router": "3",
    "react-router-bootstrap": "^0.23.3",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.4",
    "react-timeago": "^3.4.3",
    "reactstrap": "^5.0.0-alpha.3",
