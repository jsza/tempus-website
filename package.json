{
  "name": "tempus-website",
  "version": "0.0.0",
  "description": "Tempus Jump Network website client.",
  "main": "index.js",
  "scripts": {
    "watch": "npm run watch-js & npm run watch-styl",
    "build": "npm run build-js && npm run build-styl",
    "build-js": "webpack --progress --colors --display-error-details --config src/js/webpack/prod.config.js",
    "watch-js": "webpack --progress --colors --watch --config src/js/webpack/dev.config.js",
    "watch-styl": "stylus -u autoprefixer-stylus -w src/stylus/index.styl -o static/css/styles.css",
    "build-styl": "stylus -c -u autoprefixer-stylus src/stylus/index.styl -o static/css/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsza/tempus-website.git"
  },
  "author": "jayess",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsza/tempus-website/issues"
  },
  "dependencies": {
    "autobahn": "^0.9.7",
    "autoprefixer-stylus": "^0.8.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "classnames": "^2.1.5",
    "css-loader": "^0.19.0",
    "history": "^1.12.4",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "react": "^0.14.0",
    "react-bootstrap": "^0.27.1",
    "react-document-title": "^2.0.1",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "react-router-bootstrap": "^0.19.2",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.3",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "steamidconvert": "^0.2.4",
    "stylus": "^0.52.4",
    "stylus-loader": "^1.3.1",
    "superagent": "^1.4.0",
    "webpack": "^1.12.2"
  }
}
