parser: 'babel-eslint'
rules:
  "react/jsx-uses-vars":
    - 2
  quotes:
    - 2
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - never
  no-unused-vars:
    - 2
    - args: 'none'
env:
  es6: true
  browser: true
extends:
  - 'eslint:recommended'
  - 'plugin:react/recommended'
plugins:
  - react
